					<TABLE>
						<%Iterate x="[http:session] .attributeNames" %>
							<TR>
								<TD><%=./%></TD>
								<TD><%=[http:session] .getAttribute(.toString()) /%></TD>
							</TR>
						<%/Iterate%>
					</TABLE>
				</TD>
		 	</TR>

			<TR>
