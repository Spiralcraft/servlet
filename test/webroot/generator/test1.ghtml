<HTML>
	<BODY>
	
		<%@include resource="header.ghtml"/%>
		
		<H1>[http:request]</H1>
		<TABLE border="1">
			<TR>
				<TH>Expression</TH>
				<TH>Result</TH>
			</TR>

			<TR>
				<TD>[http:request] .characterEncoding</TD>
				<TD><%=[http:request] .characterEncoding /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .contentLength</TD>
				<TD><%=[http:request] .contentLength /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .contentType</TD>
				<TD><%=[http:request] .contentType /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .inputStream</TD>
				<TD><%=[http:request] .inputStream /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .locale</TD>
				<TD><%=[http:request] .locale /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .locales
				</TD>
				<TD>
					<TABLE>
						<%Iterate x="[http:request] .locales" %>
							<TR>
								<TD><%=./%></TD>
							</TR>
						<%/Iterate%>
					</TABLE>
				</TD>
		 	</TR>

			<TR>
				<TD>[http:request] .parameterNames
						<BR>[http:request] .getParameter(.toString())
				</TD>
				<TD>
					<TABLE>
						<%Iterate x="[http:request] .parameterNames" %>
							<TR>
								<TD><%=./%></TD>
								<TD>
									<%Iterate 
										x="[http:request] .getParameterValues(.toString())"
										%>
										[<%=./%>]<BR/>
									<%/Iterate%>
								</TD>
							</TR>
						<%/Iterate%>
					</TABLE>
				</TD>
		 	</TR>

			<TR>
				<TD>[http:request] .protocol</TD>
				<TD><%=[http:request] .protocol /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .remoteAddr</TD>
				<TD><%=[http:request] .remoteAddr /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .remoteHost</TD>
				<TD><%=[http:request] .remoteHost /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .scheme</TD>
				<TD><%=[http:request] .scheme /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .serverName</TD>
				<TD><%=[http:request] .serverName /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .serverPort</TD>
				<TD><%=[http:request] .serverPort /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .secure</TD>
				<TD><%=[http:request] .secure /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .attributeNames
						<BR>[http:request] .getAttribute(.toString())
				</TD>
				<TD>
					<TABLE>
						<%Iterate x="[http:request] .attributeNames" %>
							<TR>
								<TD><%=./%></TD>
								<TD><%=[http:request] .getAttribute(.toString()) /%></TD>
							</TR>
						<%/Iterate%>
					</TABLE>
				</TD>
		 	</TR>

			<TR>
				<TD>[http:request] .authType</TD>
				<TD><%=[http:request] .authType /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .contextPath</TD>
				<TD><%=[http:request] .contextPath /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .cookies</TD>
				<TD>
					<TABLE border=1>
						<TR>
							<TH>.name</TH>
							<TH>.value</TH>
							<TH>.comment</TH>
							<TH>.domain</TH>
							<TH>.maxAge</TH>
							<TH>.path</TH>
							<TH>.secure</TH>
							<TH>.version</TH>
						</TR>
						
						<%Iterate x="[http:request] .cookies" %>
							<TR>
								<TD><%=.name/%></TD>
								<TD><%=.value/%></TD>
								<TD><%=.comment/%></TD>
								<TD><%=.domain/%></TD>
								<TD><%=.maxAge/%></TD>
								<TD><%=.path/%></TD>
								<TD><%=.secure/%></TD>
								<TD><%=.version/%></TD>
							</TR>
						<%/Iterate%>
						
					</TABLE>
				</TD>
		 	</TR>

			<TR>
				<TD>[http:request] .headerNames
						<BR>[http:request] .getHeader(.)
				</TD>
				<TD>
					<TABLE>
						<%Iterate x="[http:request] .headerNames" %>
							<TR>
								<TD><%=./%></TD>
								<TD><%=[http:request] .getHeader(.toString()) /%></TD>
							</TR>
						<%/Iterate%>
					</TABLE>
				</TD>
		 	</TR>

			<TR>
				<TD>[http:request] .method</TD>
				<TD><%=[http:request] .method /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .pathInfo</TD>
				<TD><%=[http:request] .pathInfo /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .pathTranslated</TD>
				<TD><%=[http:request] .pathTranslated /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .queryString</TD>
				<TD><%=[http:request] .queryString /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .remoteUser</TD>
				<TD><%=[http:request] .remoteUser /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .requestedSessionId</TD>
				<TD><%=[http:request] .requestedSessionId /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .requestURI</TD>
				<TD><%=[http:request] .requestURI /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .requestURL</TD>
				<TD><%=[http:request] .requestURL /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .servletPath</TD>
				<TD><%=[http:request] .servletPath /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .userPrincipal</TD>
				<TD><%=[http:request] .userPrincipal /%></TD>
		 	</TR>
		 	
			<TR>
				<TD>[http:request] .requestedSessionIdFromCookie</TD>
				<TD><%=[http:request] .requestedSessionIdFromCookie /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .requestedSessionIdFromURL</TD>
				<TD><%=[http:request] .requestedSessionIdFromURL /%></TD>
		 	</TR>

			<TR>
				<TD>[http:request] .requestedSessionIdValid</TD>
				<TD><%=[http:request] .requestedSessionIdValid /%></TD>
		 	</TR>
	  </TABLE>
	  
	  <H1>[http:session]</H1>
		<TABLE border="1">
			<TR>
